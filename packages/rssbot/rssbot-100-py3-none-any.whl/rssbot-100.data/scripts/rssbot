from rssbot import Fetcher

from rssbot.irc import IRC


class CLI(CLI):

    def raw(self, txt):
        cprint(txt)


class Console(Shell):

    def raw(self, txt):
        cprint(txt)


def main():
    Callbacks.add("command", dispatch)
    i = IRC()
    i.start()
    f = Fetcher()
    f.start()
    wait()


main()
