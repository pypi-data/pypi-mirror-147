<odoo>
  <data>

    <record id="view_vh_contract_window" model="ir.actions.act_window">
      <field name="name">Contracts</field>
      <field name="res_model">contract.contract</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record id="view_vh_contract_form" model="ir.ui.view">
      <field name="name">vh.contract.contract.form.inherit</field>
      <field name="model">contract.contract</field>
      <field name="type">form</field>
      <field name="inherit_id" ref="contract.contract_contract_form_view"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='journal_id']" position="after">
          <field name="account_receivable_id"/>
        </xpath>
      </field>
    </record>

    <menuitem name="Contracts" id="vh.contract_menu" 
      parent="vh.menu_root" action="view_vh_contract_window" sequence="2"/>
  </data>
</odoo>