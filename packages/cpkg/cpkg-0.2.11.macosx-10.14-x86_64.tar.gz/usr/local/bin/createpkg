#!/Applications/Xcode.app/Contents/Developer/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'cpkg==0.2.11','console_scripts','createpkg'
__requires__ = 'cpkg==0.2.11'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cpkg==0.2.11', 'console_scripts', 'createpkg')()
    )
